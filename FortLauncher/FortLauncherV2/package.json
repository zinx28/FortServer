{
  "name": "fortlauncherv2",
  "version": "0.0.1",
  "description": "A Fortnite Launcher",
  "main": "./out/main/index.js",
  "author": "zinx",
  "build": {
    "directories": {
      "output": "dist"
    },
    "appId": "FortLauncher",
    "productName": "FortLauncher",
    "win": {},
    "asar": true,
    "files": [
      "out/**/*",
      "resources/**/*"
    ],
    "protocols": [
      {
        "name": "FortLauncher",
        "schemes": [
          "FortLauncher"
        ]
      }
    ]
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "obfuscate": "node obfuscate.js",
    "build": "npm run typecheck && electron-vite build && npm run obfuscate",
    "builddev": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win2": "npm run builddev && electron-builder --win",
    "build:win": "npm run build && electron-builder --win"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "axios": "^1.13.2",
    "discord-rpc": "^4.0.1",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.6.2",
    "ini": "^6.0.0",
    "regedit": "^5.1.4"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@rushstack/eslint-patch": "^1.15.0",
    "@types/discord-rpc": "^4.0.10",
    "@types/ini": "^4.1.1",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "javascript-obfuscator": "^4.1.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.4"
  }
}
